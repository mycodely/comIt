// Crear en una carpeta 2 archivos, en uno la fX sumar, y en otro va a requerir la función sumar,
// y ejecutarlo con 2 números que requiera por parámetro

function suma(v1,v2){
    console.log(v1+v2);
}

module.exports = suma;

